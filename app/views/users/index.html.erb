<div class="row">
  <div class="col-lg-3 col-md-3">
    <div class="well well-lg">
      <h2>Users</h2>
      <ul>
        <% @users.each do |user| %>
            <li><%= link_to user.email, user_path(user.id) %></li>
        <% end %>
      </ul>
    </div>
  </div>
  <div class="col-lg-6 col-md-6 ">
    <div class="well well-lg">
      <h2>Tweets</h2>
      <a class="btn btn-large btn-default" href="#"><i class="glyphicon glyphicon-edit"></i> New Tweet</a>
    </div>
  </div>
</div>
